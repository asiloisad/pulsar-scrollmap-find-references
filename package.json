{
  "name": "scrollmap-find-references",
  "description": "Show find-references markers on the scrollbar",
  "repository": "https://github.com/asiloisad/pulsar-scrollmap-find-references",
  "keywords": [
    "scrollmap",
    "find-references",
    "references",
    "marker"
  ],
  "version": "0.2.0",
  "license": "MIT",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "providedServices": {
    "scrollmap": {
      "versions": {
        "1.0.0": "provideScrollmap"
      }
    }
  },
  "configSchema": {
    "threshold": {
      "order": 1,
      "type": "integer",
      "title": "Threshold",
      "description": "Hide markers if count exceeds this value. Zero means no limit.",
      "minimum": 0,
      "default": 0
    }
  }
}
